func fib (int n) -> int {
    if n < 0 {
        return 0
    } elif (n == 0) \/ ( n == 1) {
         return 1
    }
    return fib(n-1) + fib(n-2)
}

func fact(int n) -> int {
    if n < 0 {
        return 0
    }
    if n == 1 {
        return 1
    }
    return n * fact(n-1)
}


